<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.infosmart.DwmisMisTypeMapper">
	<resultMap id="DwmisMisTypeMap" type="DwmisMisType">
		<result property="typeId" column="type_id" javaType="java.lang.Integer"
			jdbcType="INTEGER" />
		<result property="groupId" column="group_id" javaType="java.lang.Integer"
			jdbcType="INTEGER" />
		<result property="typeName" column="type_name" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result property="detail" column="detail" javaType="java.lang.String"
			jdbcType="VARCHAR" />
	</resultMap>
	<select id="listDwmisMisTypesByGroupId" parameterType="DwmisMisType"
		resultMap="DwmisMisTypeMap">
		select type_id,group_id,type_name,detail
		from dwmis_mis_type
		where
		group_id =#{groupId}
	</select>
</mapper>