#set( $layout = 'home/layout/null.vm' )
<settings>
   <font>微软雅黑</font>
  <hide_bullets_count>0</hide_bullets_count>
  <start_on_axis>0</start_on_axis>
  <rescale_on_hide>0</rescale_on_hide>
   <vertical_lines>
    <width>59</width>
    <clustered>1</clustered>
  </vertical_lines>
  <decimals_separator>.</decimals_separator>
  <background>
    <color>EFF0F2</color>
    <alpha>90</alpha>
    <border_alpha>0</border_alpha>
  </background>
  <plot_area>
    <margins>
      <top>30</top>
      <left>50</left>
      <right>10</right>
      <bottom>45</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>0</alpha>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>0</alpha>
    </y_left>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>000000</color>
    </x>
    <y_left>
      <width>1</width>
      <color>000000</color>
    </y_left>
  </axes>
  
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_enabled>0</x_balloon_enabled>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <legend>
    <enabled>0</enabled>
  </legend>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>

  <graphs>
    <graph gid='0'>
      <title>Anomaly</title>
      <color>80A935</color>
      <color_hover>FF0F00</color_hover>
      <line_alpha>0</line_alpha>
      <vertical_lines>1</vertical_lines>
      <balloon_color>80A935</balloon_color>
      <balloon_text_color>000000</balloon_text_color>
      <bullet>round</bullet>
	  <balloon_text>{description}{value}</balloon_text>
    </graph>
    <graph gid='1'>
      <title>Smoothed</title>
      <color>00FF00</color>
      <color_hover>000000</color_hover>
      <line_alpha>0</line_alpha>
      <fill_color>FFFFFF</fill_color>
      <balloon_color>0085AB</balloon_color>
      <balloon_text_color>000000</balloon_text_color>
	  <balloon_text>{description}{value}</balloon_text>
      <bullet>round</bullet>
      <bullet_size>10</bullet_size>
      <bullet_color>0085AB</bullet_color>
      <bullet_alpha>100</bullet_alpha>
    </graph>
    <graph gid='111'>
      <title>22345</title>
      <color>000000</color>
      <color_hover>CC0000</color_hover>
      <line_alpha>0</line_alpha>
      <balloon_color>AD504C</balloon_color>
      <balloon_text_color>000000</balloon_text_color>
	  <balloon_text>{description}{value}</balloon_text>
      <bullet>round_outlined</bullet>
      <bullet_color>AD504C</bullet_color>
      <bullet_alpha>100</bullet_alpha>
	  <bullet_size>12</bullet_size>
    </graph>
  </graphs>


  <labels>
    <label lid='0'>
      <text><![CDATA[<b>${unit}</b>]]></text><y>5</y><x>15</x>
      <text_size>14</text_size>
      <text_color>666666</text_color>
      <text_font>微软雅黑</text_font>
    </label>
  </labels>
  <strings>
      <no_data>数据正在准备中，请稍后再试。</no_data>
      <export_as_image>数据正在准备中，请稍后再试。</export_as_image>
      <error_in_data_file>数据正在准备中，请稍后再试。</error_in_data_file>
      <collecting_data>数据正在准备中，请稍后再试。</collecting_data>
      <wrong_zoom_value>数据正在准备中，请稍后再试。</wrong_zoom_value>
  </strings>
</settings>
