    <value>
      <tick_length>1</tick_length>
      <width>1</width>
      <color>E7E7E7</color>
      <alpha>7</alpha>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <legend>
    <enabled>0</enabled>
  </legend>
  <angle>60</angle>
  <column>
    <width>50</width>
    <spacing>8</spacing>
    <balloon_text>{title}: {value} %</balloon_text>
