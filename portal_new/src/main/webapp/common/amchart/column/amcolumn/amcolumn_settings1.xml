<settings>
  <data_type>csv</data_type>
  <background>
    <alpha>100</alpha>
    <border_alpha>20</border_alpha>
  </background>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
      <approx_count>6</approx_count>
      <fill_alpha>100</fill_alpha>
    </value>
  </grid>
  <axes>
    <category>
      <tick_length>0</tick_length>
      <width>0</width>
      <color>99FF00</color>
      <alpha>6</alpha>
