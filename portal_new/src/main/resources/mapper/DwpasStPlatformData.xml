<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.infosmart.mapper.DwpasStPlatFormData">
	<resultMap id="DwpasStPlatformDataMap" type="DwpasStPlatformData">
		<result property="id" column="ID" />
		<result property="reportDate" column="REPORT_DATE" />
		<result property="taobaoRate" column="wap_rate" />
		<result property="taobaoRateTrend" column="wap_rate_trend" />
		<result property="outRate" column="OUT_RATE" />
		<result property="outRateTrend" column="OUT_RATE_TREND" />
		<result property="innerRate" column="INNER_RATE" />
		<result property="innerRateTrend" column="INNER_RATE_TREND" />
		<result property="gmtCreate" column="GMT_CREATE" />
		<result property="gmtModified" column="GMT_MODIFIED" />
	</resultMap>

	<select id="queryPlatFormUserByDate" resultMap="DwpasStPlatformDataMap">
		select id, report_date, wap_rate, wap_rate_trend, out_rate,
		out_rate_trend, inner_rate, inner_rate_trend, gmt_create, gmt_modified
		from dwpas_st_platform_data where (report_date = #{value})
	</select>

</mapper>